<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodApp">
        <activity
            android:name=".Add_To_Cart"
            android:exported="false" />
        <activity
            android:name=".Help"
            android:exported="false" />
        <activity
            android:name=".Give_Rating"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OpenActivity" />
        <activity android:name=".HomeActivity"/>
        <activity android:name=".LogIn" />
        <activity android:name=".SignupActivity"/>
        <activity android:name=".My_Cart"/>
        <activity android:name=".Add_To_Cart1"/>
        <activity android:name=".Add_To_Cart2"/>
        <activity android:name=".Add_To_Cart3"/>
        <activity android:name=".OrderConfirmed"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>